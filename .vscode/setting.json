{
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.egg-info": true,
        "**/*.meta": true
      },
      "autoDocstring.docstringFormat": "numpy",
      "files.eol": "\n",
      "files.trimTrailingWhitespace": true,
      "[python]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
        "editor.rulers": [88]
      },
      "python.formatting.provider": "black",
      "python.linting.enabled": true,
      "python.linting.pycodestyleEnabled": false,
      "python.linting.flake8Enabled": true,
      "python.linting.mypyEnabled": true,
      "rewrap.wrappingColumn": 79,
      "files.associations": {
        "tuple": "cpp",
        "iostream": "cpp",
        "functional": "cpp",
        "array": "cpp",
        "hash_map": "cpp",
        "hash_set": "cpp",
        "atomic": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "csignal": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "optional": "cpp",
        "string_view": "cpp",
        "typeindex": "cpp",
        "valarray": "cpp",
        "complex": "cpp",
        "core": "cpp",
        "specialfunctions": "cpp",
        "condition_variable": "cpp",
        "*.ipp": "cpp",
        "strstream": "cpp",
        "bitset": "cpp",
        "cinttypes": "cpp",
        "codecvt": "cpp",
        "rope": "cpp",
        "slist": "cpp",
        "future": "cpp",
        "scoped_allocator": "cpp",
        "shared_mutex": "cpp",
        "algorithm": "cpp",
        "set": "cpp",
        "csetjmp": "cpp",
        "cfenv": "cpp",
        "__config": "cpp",
        "__nullptr": "cpp",
        "__locale": "cpp",
        "ios": "cpp",
        "tensor": "cpp",
        "iterator": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "alignedvector3": "cpp",
        "__split_buffer": "cpp",
        "deque": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "map": "cpp",
        "queue": "cpp",
        "regex": "cpp",
        "stack": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "__mutex_base": "cpp",
        "random": "cpp",
        "__functional_03": "cpp",
        "hashtable": "cpp",
        "fft": "cpp",
        "*.expression": "cpp",
        "superlusupport": "cpp",
        "adolcforward": "cpp",
        "autodiff": "cpp",
        "bvh": "cpp",
        "threadpool": "cpp",
        "eulerangles": "cpp",
        "kroneckerproduct": "cpp",
        "mprealsupport": "cpp",
        "numericaldiff": "cpp",
        "openglsupport": "cpp",
        "splines": "cpp",
        "*.evaluator": "cpp",
        "*.traits": "cpp",
        "*.in": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__tree": "cpp",
        "__tuple": "cpp",
        "__functional_base": "cpp",
        "__functional_base_03": "cpp",
        "cassert": "cpp",
        "dense": "cpp",
        "geometry": "cpp",
        "*.inc": "cpp",
        "variant": "cpp",
        "numeric": "cpp",
        "any": "cpp",
        "xstring": "cpp",
        "locale": "cpp",
        "xfacet": "cpp",
        "xhash": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocbuf": "cpp",
        "xlocinfo": "cpp",
        "xlocmes": "cpp",
        "xlocmon": "cpp",
        "xlocnum": "cpp",
        "xloctime": "cpp",
        "xmemory": "cpp",
        "xmemory0": "cpp",
        "xstddef": "cpp",
        "xtr1common": "cpp",
        "xtree": "cpp",
        "xutility": "cpp",
        "xthread": "cpp",
        "memory_resource": "cpp",
        "concepts": "cpp",
        "filesystem": "cpp",
        "*.js": "javascript",
        "*.jsx": "javascriptreact",
        "__string": "cpp"
      },
      "git.ignoreLimitWarning": true,
      "typescript.tsdk": "node_modules/typescript/lib",
      "eslint.packageManager": "yarn",
      "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "vue"],
      "python.pythonPath": "${env.CONDA_PREFIX}/bin/python",
      "eslint.format.enable": true,
      "prettier.useEditorConfig": false,
      "[typescript]": {
        "editor.formatOnSave": true
      },
      "[typescriptreact]": {
        "editor.formatOnSave": true
      },
      "[javascript]": {
        "editor.formatOnSave": true
      },
      "[javascriptreact]": {
        "editor.formatOnSave": true
      },
      "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
      },
      "vetur.validation.template": false,
      "emeraldwalk.runonsave": {
        "commands": [
          {
            "match": "\\.py$",
            "cmd": "make format-python-file FILE=${file}"
          }
        ]
      },
      "python.formatting.blackPath": "pyfmt"
    }